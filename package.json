{
  "scripts": {
    "precommit": "lint-staged",
    "build:dev": "cross-env NODE_ENV=development babel client/ -d build/dev/",
    "build:prod": "cross-env NODE_ENV=production babel client/ -d build/prod/",
    "build:prod-legacy": "cross-env NODE_ENV=production-legacy babel client/ -d build/prod-legacy/",
    "build": "run-p build:dev build:prod build:prod-legacy",
    "eslint": "eslint client",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "*.{js,json}": ["prettier-eslint --parser flow --write", "git add"]
  },
  "dependencies": {
